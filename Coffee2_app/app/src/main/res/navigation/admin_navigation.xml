        tools:layout="@layout/fragment_admin_events" />

    <fragment
        android:id="@+id/admin_navigation_facilities"
        android:name="com.example.coffee2_app.Admin_ui.browseFacilities.BrowseFacilitiesFragment"
        android:label="@string/facilities"
        tools:layout="@layout/fragment_admin_facilities" />

    <fragment
        android:id="@+id/admin_navigation_images"
        android:name="com.example.coffee2_app.Admin_ui.browseImages.BrowseImagesFragment"
